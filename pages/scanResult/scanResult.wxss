@import "../../lib/common.wxss";
.form-box{
	margin: 48rpx 56rpx;
}
.form-tit{
  font-size: 40rpx;
  text-align: left;
  font-family: PingFangSC-bold;
  margin-bottom: 20rpx;
}
.form-tips{
  color: rgba(153, 153, 153, 1);
  font-size: 24rpx;
  text-align: left;
  font-family: PingFangSC-regular;
  margin-top: 20rpx;
}

.form-btn{
	margin-top: 66rpx;
}
.input-unit{
	position: absolute;
	right: 40rpx;
	z-index: 2;
}

.name-padding{
	padding: 54rpx 0rpx 40rpx 0rpx;
}
.card-info-sub{
  display: inline-block;
  padding: 0rpx 12rpx;
  height: 44rpx;
  line-height: 44rpx;
  border-radius: 22rpx;
  background-color: #ffffff;
  text-align: center;
  color: #101010;
  font-size: 24rpx;
  margin-left: 18rpx;
}
.card-reasult .card-base-label{
  margin-right: 244rpx;
}
.swiper-box{
  padding: 20rpx 0rpx;
  height: 460rpx;
}
.zoom-out {
  transition: all 0.7s ease-out 0s;
  background-color: rgba(24, 125, 240, .6);
}
.zoom-in {
  transition: all 0.7s ease-in 0s;
  background-color: rgba(24, 125, 240, 1);
}