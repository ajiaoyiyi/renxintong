@import "../../../lib/common.wxss";
.form-box{
	margin: 0 auto;
}
.form-input{
	width: 576rpx;
	height: 68rpx;
	line-height: 68rpx;
	border-radius: 34rpx;
	text-align: center;
	border: 1px solid #BBBBBB;
}
.form-item .clear-icon{
    position: relative; 
}
.form-item .clear-icon::after{
	position: absolute;
	content: '';
	top: 10rpx;
    right: 36rpx;
    display: block;
	background-image:url('data:image/png;base64,');
	background-size: 50rpx;
	width:50rpx;
    height:50rpx;
}
.form-btn{
	margin-top: 476rpx;
}